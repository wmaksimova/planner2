<Window x:Class="planner3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:planner3"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas Background="#FAC3F0">
            <Image x:Name="calendar" Source="Resources/new_calendar.png" Width="30" Height="30" Canvas.Left="10" Canvas.Top="10"/>
            <ListBox x:Name="dateList" Width="158" Height="24" Canvas.Left="40" Canvas.Top="16" Background="#FAC3F0" RenderTransformOrigin="0.499,0.453"/>
        </Canvas>
    </Grid>
</Window>
